pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/Varunkaushik2004/DEVOPS.git'
            }
        }
        stage('Build') {
            steps {
                bat  'echo "Build Step"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Testing Step"'
            }
        }
    }
}
